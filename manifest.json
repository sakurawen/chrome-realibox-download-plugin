{
  "manifest_version": 3,
  "name": "realibox-donwload-chrome-plugin",
  "version": "1.0",
  "description": "realihub hub下载离线包插件",
  "devtools_page": "dist/devtool.html",
  "permissions": [
    "downloads",
    "scripting",
    "activeTab",
    "tabs"
  ],
  "action": {},
  "web_accessible_resources": [
    {
      "resources": [
        "plugin/inject/historyApiReplace.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "*://hub.realibox.com/project/*"
      ],
      "js": [
        "plugin/index.js"
      ]
    }
  ]
}
