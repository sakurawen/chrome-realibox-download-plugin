{
  "manifest_version": 3,
  "name": "realibox-donwload-chrome-plugin",
  "version": "1.0",
  "description": "realihub hub下载离线包插件",
  "devtools_page": "dist/index.html",
  "permissions": [
    "notifications",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://hub.realibox.com/project/*"
      ],
      "js": [
        "plugin/index.js"
      ]
    }
  ]
}
